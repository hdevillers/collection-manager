{{ form_start(form) }}
    <fieldset>
        <legend>General informations</legend>
        {{ form_row(form.genus) }}
        {{ form_row(form.species) }}
        {{ form_row(form.type) }}
        {{ form_row(form.name) }}
        {{ form_row(form.comment) }}
        {{ form_row(form.sequenced) }}
        {{ form_row(form.deleted) }}
    </fieldset>

    <fieldset>
        <legend>Tubes</legend>
        {{ form_row(form.tubes) }}
    </fieldset>

    {% block specialFields %}
    {% endblock %}

    {{ form_widget(form) }}

<input type="submit" class="btn btn-default pull-right" value="Save">
{{ form_end(form) }}

{%block map %}
{% endblock %}
