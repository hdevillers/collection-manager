{% extends "strain/wild/strainFormDisplay.html.twig" %}

{% block title %}
    Edit a wild strain - {{ parent() }}
{% endblock %}

{% block breadcrumb %}
    {{ parent() }}
    <li><a href="{{ path('strain_index') }}">Strains</a></li>
    <li><a href="{{ path('strain_wild_view', { id: strain.id }) }}">{{ strain.systematicName }} - {{ strain.usualName }}</a></li>
    <li class="active">Edit</li>
{% endblock %}

{% block h2 %}
    <h2>Edit a wild strain: {{ strain.fullName }}</h2>
{% endblock %}
