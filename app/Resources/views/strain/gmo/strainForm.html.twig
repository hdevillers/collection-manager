{% embed 'strain/skeleton/strainForm.html.twig' %}
    {% block specialFields %}
        <fieldset>
            <legend>Biological informations</legend>
            {{ form_row(form.description) }}
            {{ form_row(form.genotype) }}
        </fieldset>

        <fieldset>
            <legend>Plasmids</legend>
            {{ form_row(form.strainPlasmids) }}
        </fieldset>

        <fieldset>
            <legend>Parents</legend>
            {{ form_row(form.parents) }}
        </fieldset>
    {% endblock %}
{% endembed %}
